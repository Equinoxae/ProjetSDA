int d = MatVal2(dist,h-1,l-1);
	int p = (h-1)*(l-1);

	while(d>=0){
		SetPointGraphe((int)(p-l)/l,(int)(p-l)%l, "bleu");	
		if(!p){
			// fin
		}
		else if(MatVal(dist,p-1) == d-1){
			p = p-1;
		}
		else if(MatVal(dist,p-l) == d-1){
			p = p-l;
		}	
		else if(MatVal(dist,p+1) == d-1){
			p = p+1;
		}	
		else if(MatVal(dist,p+l) == d-1){
			p = p+l;
		}
		else{
			printf("erreur chemin\n");
		}	
		d--;	
	}
